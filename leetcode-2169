#include<iostream>
using namespace std;
class Solution {
public:
    int countOperations(int num1, int num2) {
        int count=0;
          while(num1>0 && num2>0){
            if(num1>=num2){
                num1=num1-num2;
            }
            else if(num2>num1){
                num2=num2-num1;
            }
            count++;
          }
          return count;
    }

    int main(){
        Solution s1;
       s1. countOperations(2, 3);
        return 0;
    }
};
