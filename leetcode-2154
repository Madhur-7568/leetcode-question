class Solution {
public:
    int findFinalValue(vector<int>& nums, int original) {
       sort(nums.begin(),nums.end());
        int n=nums.size();
        for(int i=0;i<n;i++){
            if(nums[i]==original){
                original=2*original;
            }
        }
        return original;
    }
    int main(){
        vector<int>nums={5,3,6,1,12};
        findFinalValue(nums, 3);
        return 0;
    }
};
