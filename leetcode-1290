class Solution {
public:
    int getDecimalValue(ListNode* head) {
        int size=0;
       ListNode* temp=head;
        while(head!=NULL){
            size++;
            head=head->next;
        }
        int pow=1<<(size-1);
        int num=0;
        while(temp!=NULL){
            num=num+(temp->val)*pow;
            pow=pow/2;
            temp=temp->next;
        }
        return num;
    }
};
